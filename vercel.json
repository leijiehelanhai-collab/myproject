{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_CONTRACT_ADDRESS": "@contract_address",
    "REACT_APP_TEST_TOKEN_1": "@test_token_1",
    "REACT_APP_TEST_TOKEN_2": "@test_token_2"
  }
}
